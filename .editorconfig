# http://EditorConfig.org
# top-most EditorConfig file
root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = false
indent_style = tab

[*.{astro,cjs,css,js,jsx,mjs,ts,tsx}]
charset = utf-8
insert_final_newline = true
indent_size = 4

[*.{md,mdx}]
trim_trailing_whitespace = false
indent_style = space

[{package.json,statichost.yml}]
charset = utf-8
insert_final_newline = true
indent_size = 2
indent_style = space
